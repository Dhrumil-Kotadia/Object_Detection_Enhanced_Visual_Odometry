    "import json\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "plt.figure(facecolor=\"black\")\n",
    "ax = plt.axes()\n",
    "ax.set_facecolor(\"black\")\n",
    "\n",
    "F = open(\"../Output/Poses_temp.json\", \"r\")\n",
    "Data = json.load(F)\n",
    "F.close()\n",
    "\n",
    "for key in Data:\n",
    "    plt.plot(Data[key][0], -Data[key][2], \"b.\")\n",
    "\n",
    "plt.show()"
   ]
